{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://open-rmf.org/rmf_api_msgs/fleet_log/0.1",
  "title": "Fleet State",
  "description": "The log of a fleet",
  "type": "object",
  "properties": {
    "name": { "type": "string" },
    "log": {
      "description": "Log for the overall fleet",
      "type": "array",
      "items": { "$ref": "log_entry.schema.json" }
    },
    "robots": {
      "description": "Dictionary of logs for the individual robots. The keys (property names) are the robot names.",
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": { "$ref": "log_entry.schema.json" }
      }
    }
  }
}
